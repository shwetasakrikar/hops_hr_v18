@model IEnumerable<GeneratorBase.MVC.Models.T_Country>
@{
    ViewBag.Title = "Country List";
	Layout ="~/Views/Shared/_Layout.cshtml";
	string templatename = ("~/Views/T_Country/" + ViewBag.TemplatesName + ".cshtml").Replace("?IsAddPop=true", "");
	 var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "T_Country");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "Country";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title" >
            <i class="glyphicon glyphicon-thumbs-up text-primary" style="margin-bottom:-20px;"></i> @EntityDisplayName  <i class="glyphicon glyphicon-chevron-right small"></i>  <span>List</span>
</h1>
    </div>
</div>
<script>
    $(document).ready(function () {
        ClickFilterBtn();
    });
</script>
<div id="dvT_CountryFilter"></div>
@Html.Partial(templatename, Model)

