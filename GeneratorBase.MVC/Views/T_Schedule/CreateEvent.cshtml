@model Dictionary<string, string>
@{
    Layout = null;

}
@if (Model.Count() == 1)
{
    <script>
        $(document).ready(function () {
            $('a', "#dvCreateEvent").click();
        });
    </script>
}
<div class="panel panel-default AppForm" id="dvCreateEvent">
    <div class="panel-body">
        <br />
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="col-sm-10 col-md-10 col-xs-12">
                    <div class='form-group'>
                        @foreach (var item in Model)
                        {

                            <a data-original-title='@item.Value' data-placement="top" style="padding:3px 5px;" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Create", item.Key, new { UrlReferrer = Request.UrlReferrer, AssociatedType = ViewData["AssociatedType"], HostingEntityName = @Convert.ToString(ViewData["HostingEntity"]), HostingEntityID = @Convert.ToString(ViewData["HostingEntityID"]), viewtype = Convert.ToString(ViewBag.TemplatesName), startDate = ViewData["startDate"], slotID = ViewData["slotID"] }, null) + "');")>
                                <input type="radio" name="opinions" id="radio-1" /> Add @item.Value
                            </a>
                            <br />
                        }
                    </div>
                </div>
            </div>

        </div>
        <button id="CancelQuickAdd" type="button" class="btn btn-default btn-sm pull-right" data-dismiss="modal" aria-hidden="true">Cancel</button>
    </div>
</div>

