@{
    ViewBag.Title = "Concurrency Alert";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title">
            <i class="glyphicon glyphicon-exclamation-sign text-primary"></i> Concurrency Alert 
        </h1>
        <label id="ErrMsg" class="text-primary small pull-right" style="color:red; vertical-align:middle; font-weight:100;"></label>
        <p class="text-primary">@ViewData["ConcurrencyMessage"]</p>
        @Html.ActionLink("Get updated record", "ConcurrencyButton", new { UrlReferrer = ViewData["ConcurrencyReferrer"] },new {@class = "btn btn-default btn-sm" })
    </div>
    <!-- /.col-lg-12 -->
</div>
