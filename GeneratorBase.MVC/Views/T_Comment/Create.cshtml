@model GeneratorBase.MVC.Models.T_Comment
@{
    ViewBag.Title = "Create Comment";
	var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "T_Comment");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "Comment";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title" style="margin-bottom:10px">
		@{
                var hostingValue = "";
                if (ViewBag.DisplayVal != null)
                {
                    hostingValue = " - " + ViewBag.DisplayVal;
                }
            }
            <i class="glyphicon glyphicon-save text-primary"></i> @EntityDisplayName  <i class="glyphicon glyphicon-chevron-right small"></i> <span> Add Comment @hostingValue</span>
			<a class="btn btn-xs pull-right" type="button" data-original-title="List View" data-placement="top" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Index", "T_Comment") + "');") alt="Search" title="List">
				<i class="glyphicon glyphicon-list"></i> List
			</a>
			</div>
	

        </h1>
        <h2 class="text-primary">@Html.DisplayFor(model => model.DisplayValue)</h2>
    </div>
    <!-- /.col-lg-12 -->
</div>
@Html.Partial("~/Views/T_Comment/CreatePartial.cshtml", Model)
