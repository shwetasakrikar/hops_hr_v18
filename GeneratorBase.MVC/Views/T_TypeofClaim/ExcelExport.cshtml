@model PagedList.IPagedList<GeneratorBase.MVC.Models.T_TypeofClaim>
@using PagedList.Mvc;
@{
    ViewBag.Title = "Index";
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", "attachment; filename=Type of Claim.xls");
}
                    <table>
                            <tr>
@if(User.CanView("T_TypeofClaim","T_ClaimTypeID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_ClaimType"?"hidden":"")>Type of Claim Facility</th>
}
@if(User.CanView("T_TypeofClaim","T_EmployeeInjuryID"))
{
		<th  @(Convert.ToString(ViewData["HostingEntity"]) == "T_EmployeeInjury"?"hidden":"")>Employee Injury</th>
}
    </tr>
@foreach (var item in Model) {
    <tr>
	@if(User.CanView("T_TypeofClaim","T_ClaimTypeID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_ClaimType"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_claimtype.DisplayValue)
        </td>
}
@if(User.CanView("T_TypeofClaim","T_EmployeeInjuryID"))
{
		<td @(Convert.ToString(ViewData["HostingEntity"]) == "T_EmployeeInjury"?"hidden":"")>
            @Html.DisplayFor(modelItem => item.t_employeeinjury.DisplayValue)
        </td>
}
                                </tr>
                            }
</table>
               
