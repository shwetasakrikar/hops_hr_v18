@model GeneratorBase.MVC.Models.CustomReports
@{
    ViewBag.Title = "Create CustomReports";
    var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "CustomReports");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "CustomReports";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title">
            @{
                var hostingValue = "";
                if (ViewBag.DisplayVal != null)
                {
                    hostingValue = " - " + ViewBag.DisplayVal;
                }
            }
            <i class="glyphicon glyphicon-save text-primary"></i> @EntityDisplayName  <i class="glyphicon glyphicon-chevron-right small"></i> <span> Add CustomReports @hostingValue</span>
            <button class="btn btn-xs btn-default" type="button" data-original-title="List View" data-placement="top" onclick=@Html.Raw("NavigateToUrl('" + Url.Action("Index", "CustomReports") + "');")>
                <i class="glyphicon glyphicon-list"></i> List
            </button>
            <div class="btn-group pull-right" style="margin-top:3px; margin-right:3px">
                <a href="#" class="btn btn-default btn-xs closeall"><i class="glyphicon glyphicon-chevron-up"></i> Collapse All</a> <a href="#" class="btn btn-default btn-xs openall"><i class="glyphicon glyphicon-chevron-down"></i> Expand All</a>
            </div>
        </h1>
    </div>
    
    <h2 class="text-primary">@Html.DisplayFor(model => model.DisplayValue)</h2>
</div>
<!-- /.col-lg-12 -->
@Html.Partial("~/Views/CustomReports/CreatePartial.cshtml", Model)
