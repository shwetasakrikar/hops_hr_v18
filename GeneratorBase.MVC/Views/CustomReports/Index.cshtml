@model IEnumerable<GeneratorBase.MVC.Models.CustomReports>
@{
    ViewBag.Title = "CustomReports List";
	Layout ="~/Views/Shared/_Layout.cshtml";
    string templatename = ("~/Views/CustomReports/IndexPartial.cshtml").Replace("?IsAddPop=true", "");
	 var EntityDisplayNameReflector = ModelReflector.Entities.FirstOrDefault(p => p.Name == "CustomReports");
    var EntityDisplayName = EntityDisplayNameReflector != null ? EntityDisplayNameReflector.DisplayName : "CustomReports";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-title">
            <i class="glyphicon glyphicon-thumbs-up text-primary" style="margin-bottom:-20px;"></i> @EntityDisplayName  <i class="glyphicon glyphicon-chevron-right small"></i>  <span>List</span>
</h1>
    </div>
</div>
<script>
    $(document).ready(function () {
        ClickFilterBtn();
    });
</script>
<div id="dvCustomReportsFilter"></div>
@Html.Partial(templatename, Model)

